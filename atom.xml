layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Gamification.cz</title>
 <link href="http://gamification.cz/atom.xml" rel="self"/>
 <link href="http://gamification.cz/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://gamification.cz/</id>
 <author>
   <name>Gamification.cz</name>
   <email>gamificationcz@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://gamification.cz{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://gamification.cz{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>